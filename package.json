{
  "name": "d-merge",
  "description": "patch",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/SARDONYX-sard/d-merge",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf ./gui/frontend/out && tauri build --features extra_fmt,tracing --no-bundle -- --profile release-no-lto",
    "build:cross": "rimraf ./gui/frontend/out && tauri build --features extra_fmt,tracing --no-bundle --runner cargo-xwin --target x86_64-pc-windows-msvc -- --profile release-no-lto",
    "build:front": "next build ./gui/frontend",
    "build:icons": "cargo tauri icon ./gui/backend/icons/icon.png",
    "dev": "tauri dev",
    "dev:front": "next ./gui/frontend",
    "fmt": "biome format --write ./ && prettier --cache --write \"**/*.{yaml,yml}\" --ignore-path ./.gitignore && cargo fmt --all",
    "lint": "next lint ./gui/frontend -c ./gui/frontend/eslint.config.mjs && biome lint ./ && cargo clippy --workspace",
    "lint:fix": "npm run fmt && biome check --write ./ && next lint ./gui/frontend --fix -c ./gui/frontend/eslint.config.mjs && cargo clippy --workspace --fix --allow-staged --allow-dirty",
    "tauri": "tauri",
    "test": "vitest run",
    "test:all": "npm test && npm run test:back",
    "test:back": " cargo test --workspace",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "6.2.0",
    "@mui/lab": "6.0.0-beta.19",
    "@mui/material": "6.2.0",
    "@mui/x-data-grid": "7.22.2",
    "@mui/x-tree-view": "7.23.0",
    "@tauri-apps/api": "2.1.1",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-fs": "2.2.0",
    "@tauri-apps/plugin-shell": "^2.2.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "i18next": "24.1.0",
    "next": "15.1.0",
    "notistack": "^3.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.0",
    "react-i18next": "15.1.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@tauri-apps/cli": "2.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/node": "22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react-swc": "3.7.2",
    "eslint": "9.16.0",
    "eslint-config-next": "15.1.0",
    "jsdom": "25.0.1",
    "prettier": "3.4.2",
    "rimraf": "^6",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "overrides": {
    "monaco-editor": "^0.49.0"
  },
  "packageManager": "npm@10.8.3"
}
